<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE xml> 
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.debug.ui.launchShortcuts">
      <shortcut
            class="com.xiting.a4e.ui.launch.LaunchShortcut"
            icon="icons/xiting_logo_icon.png"
            id="com.xiting.a4e.ui.launchShortcut"
            label="Xiting Alchemist"
            modes="run">
         <contextualLaunch>
            <enablement>
               <reference
                     definitionId="com.sap.adt.atc.ui.isCheckableByAtc">
               </reference>
            </enablement>
         </contextualLaunch>
      </shortcut>
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <e4view
            allowMultiple="false"
            category="com.xiting.a4e.ui"
            class="com.xiting.a4e.ui.views.BasicView"
            icon="icons/xiting_logo_icon.png"
            id="com.xiting.a4e.ui.basic"
            name="%e4view.basic"
            restorable="false">
      </e4view>
      <category
            id="com.xiting.a4e.ui"
            name="Xiting">
      </category>
      <e4view
            allowMultiple="false"
            category="com.xiting.a4e.ui"
            class="com.xiting.a4e.ui.views.CallStackView"
            icon="icons/xiting_logo_icon.png"
            id="com.xiting.a4e.ui.callStack"
            name="%e4view.callstack"
            restorable="false">
      </e4view>
      <e4view
            allowMultiple="false"
            category="com.xiting.a4e.ui"
            class="com.xiting.a4e.ui.views.AuthView"
            icon="icons/xiting_logo_icon.png"
            id="com.xiting.a4e.ui.authBasic"
            name="%e4view.authorityChecks"
            restorable="false">
      </e4view>
      <e4view
            allowMultiple="false"
            category="com.xiting.a4e.ui"
            class="com.xiting.a4e.ui.views.AuthSu24View"
            icon="icons/xiting_logo_icon.png"
            id="com.xiting.a4e.ui.authSu24"
            name="%e4view.authChecksSu24"
            restorable="false">
      </e4view>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="com.xiting.a4e.ui.preferences.Preferences"
            id="com.xiting.a4e.ui.preferences"
            name="Xiting Alchemist">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesScope"
            id="com.xiting.a4e.ui.scopepreferences"
            name="%page.scopePreferences">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesColumns"
            id="com.xiting.a4e.ui.columnspreferences"
            name="%page.columnsPreferences">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesAuthCheck"
            id="com.xiting.a4e.ui.authcheckpreferences"
            name="%page.authorityChecksPreferences">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesPatterns"
            id="com.xiting.a4e.ui.patternspreferences"
            name="%page.patternsPreferences">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesViews"
            id="com.xiting.a4e.ui.viewspreferences"
            name="%page.viewsPreferences">
      </page>
      <page
            category="com.xiting.a4e.ui.preferences"
            class="com.xiting.a4e.ui.preferences.PreferencesAuthSu24Check"
            id="com.xiting.a4e.ui.authcheckssu24preferences"
            name="%page.authChecksSu24Preferences">
      </page>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="com.xiting.a4e.ui.preferences.PreferenceInitialiser">
      </initializer>
   </extension>

</plugin>
